<%- include("templates/headerTemplate") %>
  <link rel="stylesheet" href="/CSS/loginStyles.css" />
  <body>
    <div class="Container">
      <%- include("templates/navTemplate") %>
      <main>
        <div>
          <h1>LOGIN</h1>
        <form name="registration" class="registartion-form" onsubmit="return formValidation()">
            <table>       
            <tr>
                <td><label for="email">Email:</label></td>
                <td><input type="email" name="email" id="emailInput" placeholder="name@sample.com" required></td>
            </tr>
            <tr>
                <td><label for="Password">Password:</label></td>
                <td><input type="password" name="password" id="passwordInput"></td>
            </tr>
            </table>
            <h3></h3>
            <td><button type="button" class="notifyButton">LOGIN</button></td>
            <P>FORGOT PASSWORD?</P>
            <P>NEW?</P>
            <td><button type="button" class="notifyButton">SIGN UP</button></td>
        </div>
      </main>
      <%- include("templates/footerTemplate") %>
    </div>
  </body>
</html>