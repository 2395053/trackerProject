<%- include("templates/headerTemplate") %>
  <link rel="stylesheet" href="/CSS/registerStyles.css" />
  <body>
    <div class="Container">
      <%- include("templates/navTemplate") %>
      <main>
        <h1>Account Profile </h1>
        <form name="registration" class="registartion-form" onsubmit="return formValidation()">
          <table>
            <tr>
              <td><label for="First Name">First Name:</label> </td>
              <td><input type="text" name="First Name" id="firstNameInput" placeholder="First Name" required></td>
            </tr>
            <tr>
                <td><label for="Last Name">Last Name:</label></td>
                <td><input type="text" name="lasName" id="lastNameInput" placeholder="Last name" required></td>
            </tr>
            <tr>
                <td><label for="date">Date OF Birth</label></td>
                <td><input type="date" name="date" id="dOBInput" required></td>
            </tr>
            <tr>
              <td><label for="email">Email:</label></td>
              <td><input type="email" name="email" id="emailInput" placeholder="name@sample.com" required></td>
            </tr>
            <tr>
              <td><label for="phoneNumber">Phone Number:</label></td>
              <td><input type="number" name="phoneNumber" id="phoneNumberInput" required placeholder="(514)1243567"></td>
            </tr>
            <tr>    
              <td><label for="User Name">UserName :</label></td>
              <td><input type="name" name="userName" id="userNameInput"></td>
            </tr>
            <tr>
                <td><label for="Password">Password:</label></td>
                <td><input type="password" name="password" id="passwordInput"></td>
            </tr>
            <tr>
              <td><button type="button" class="submit">REGISTER</button></td> 
            </tr>
          </table>
        </form>
      </main>
      <%- include("templates/footerTemplate") %>
    </div>
  </body>
</html>